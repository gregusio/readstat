{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/favicon.*", "/*.svg", "/*.png", "/*.ico"]
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
